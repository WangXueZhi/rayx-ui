{
  "name": "rayx-ui",
  "version": "3.0.0-beta.1",
  "main": "lib/index.js",
  "scripts": {
    "build:test": "cross-env NODE_ENV=development BUILD_TARGET=example webpack --progress",
    "build:prod": "cross-env NODE_ENV=production BUILD_TARGET=example webpack --progress",
    "serve": "cross-env NODE_ENV=development BUILD_TARGET=example webpack serve --hot --color",
    "open": "cross-env NODE_ENV=development BUILD_TARGET=example webpack serve --open --hot --color",
    "lib:test": "cross-env NODE_ENV=development BUILD_TARGET=lib webpack --progress",
    "lib:prod": "cross-env NODE_ENV=production BUILD_TARGET=lib webpack --progress",
    "dev": "gulp dev",
    "lib": "gulp lib",
    "build": "gulp build",
    "build:all": "gulp buildAll",
    "prepublish": "gulp buildAll",
    "create": "gulp create",
    "lint": "eslint --fix",
    "commit": "cz",
    "changelog": "conventional-changelog -p angular -i CHANGELOG.md -s -r 0",
    "publish": "npm publish && conventional-changelog -p angular -i CHANGELOG.md -s -r 0 && git add CHANGELOG.md"
  },
  "husky": {
    "hooks": {
      "pre-commit": "echo \"git commit trigger husky pre-commit hook\" && npx lint-staged",
      "commit-msg": "commitlint -e $HUSKY_GIT_PARAMS"
    }
  },
  "lint-staged": {
    "*.{ts,js,vue}": [
      "prettier --write",
      "eslint --fix"
    ]
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "dependencies": {
    "animate.css": "^4.1.1",
    "axios": "^0.21.1",
    "core-js": "^3.12.1",
